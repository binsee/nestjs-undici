{
  "name": "nestjs-undici",
  "version": "0.1.57",
  "description": "Undici utilities module based on the @nodejs/undici package 🌐",
  "homepage": "https://hebertcisco.github.io/nestjs-undici",
  "main": "index.js",
  "types": "index.d.ts",
  "engines": {
    "node": ">=18",
    "npm": ">=9"
  },
  "scripts": {
    "serve:website": "docsify serve .",
    "build": "rimraf -rf lib && tsc -p tsconfig.json",
    "format": "prettier --write \"{src,test}/**/*.ts\"",
    "lint": "eslint src --ext .js,.ts",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next",
    "test:e2e": "jest --config ./tests/jest-e2e.json --runInBand",
    "prerelease": "npm run build",
    "release": "release-it",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "devDependencies": {
    "@commitlint/cli": "17.8.1",
    "@commitlint/config-angular": "17.8.1",
    "@nestjs/common": "^11.0.1",
    "@nestjs/core": "^11.0.1",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/testing": "^11.0.1",
    "@types/jest": "29.5.14",
    "@types/node": "22.10.7",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "docsify": "^4.13.1",
    "eslint": "8.57.1",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jest": "^27.9.0",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "komit": "^1.0.5",
    "lint-staged": "13.3.0",
    "prettier": "3.4.2",
    "reflect-metadata": "0.2.2",
    "release-it": "^16.3.0",
    "rimraf": "5.0.10",
    "rxjs": "7.8.1",
    "ts-jest": "29.2.5",
    "typescript": "5.7.3"
  },
  "peerDependencies": {
    "@nestjs/common": "^7.0.0 || ^8.0.0 || ^9.0.0 || ^10.0.0 || ^11.0.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^6.0.0 || ^7.0.0",
    "undici": "^5.28.5"
  },
  "contributors": [
    {
      "name": "Hebert Cisco <hebertcisco@outlook.com>",
      "url": "https://github.com/hebertcisco",
      "author": true
    },
    {
      "name": "binsee",
      "url": "https://github.com/binsee",
      "author": false
    },
    {
      "name": "shevchenkonik",
      "url": "https://github.com/shevchenkonik",
      "author": false
    },
    {
      "name": "favelado-web",
      "url": "https://github.com/favelado-web",
      "author": false
    }
  ],
  "files": [
    "*.d.ts",
    "index.js",
    "lib",
    "docs"
  ],
  "keywords": [
    "fetch",
    "http",
    "https",
    "promise",
    "request",
    "curl",
    "wget",
    "xhr",
    "whatwg"
  ],
  "standard": {
    "env": [
      "jest"
    ],
    "ignore": [
      "src/shared/mocks"
    ]
  },
  "author": "Hebert Cisco",
  "license": "MIT",
  "url": "https://github.com/hebertcisco/nestjs-undici#readme",
  "lint-staged": {
    "*.ts": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -c .commitlintrc.json -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged",
      "prepare-commit-msg": "komit $HUSKY_GIT_PARAMS"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hebertcisco/nestjs-undici"
  },
  "bugs": {
    "url": "https://github.com/hebertcisco/nestjs-undici/issues"
  }
}
